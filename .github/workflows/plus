#!/usr/bin/python3
import sys
import statistics

def main():
    # 標準入力から数値を読み込む
    nums = [int(line) for line in sys.stdin if line.strip()]

    # 数値の統計情報を計算
    total = sum(nums)
    mean = statistics.mean(nums) if nums else 0
    median = statistics.median(nums) if nums else 0
    max_value = max(nums) if nums else 0
    min_value = min(nums) if nums else 0
    count = len(nums)

    # 統計情報を出力
    print(f"合計: {total}")
    print(f"平均: {mean}")
    print(f"中央値: {median}")
    print(f"最大値: {max_value}")
    print(f"最小値: {min_value}")
    print(f"数: {count}")

if __name__ == "__main__":
    main()

